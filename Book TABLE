CREATE TABLE [dbo].[Book](
	[BookId] [int] IDENTITY(1,1) NOT NULL primary key,
	[Title] [nvarchar](max) NULL,
	[Description] [nvarchar](max) NULL,
	[Language] [nvarchar](max) NULL,
	[PublishedYear] [nvarchar](max) NULL,
	[IsAvailable] [bit] NOT NULL,
	[IsDeleted] [bit] NOT NULL,
	[Author_AuthorId] [int] NULL FOREIGN KEY REFERENCES Author(AuthorId),
	[Publisher_PublisherId] [int] NULL  FOREIGN KEY REFERENCES Publisher(PublisherId),
	[Category_CategoryId] [int] NULL  FOREIGN KEY REFERENCES Category(CategoryId])
	)
